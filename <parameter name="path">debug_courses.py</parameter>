<parameter name="content">#!/usr/bin/env python3

import sys
import os
sys.path.append(os.path.dirname(os.path.abspath(__file__)))

from app.database.session import SessionLocal
from app.models.course import Course

def main():
    db = SessionLocal()
    
    try:
        courses = db.query(Course).all()
        
        print("Courses in database:")
        for course in courses:
            print(f"ID: {course.id}, Name: {course.name}, Code: {getattr(course, 'code', 'N/A')}")
        
        # Check if there's a way to get course by name
        chemistry_courses = db.query(Course).filter(Course.name.ilike('%chemistry%')).all()
        print(f"\nChemistry courses: {chemistry_courses}")
        
        bio_courses = db.query(Course).filter(Course.name.ilike('%bio%')).all()
        print(f"Biology courses: {bio_courses}")
        
    except Exception as e:
        print(f"Error: {e}")
    finally:
        db.close()

if __name__ == "__main__":
    main()</parameter>
